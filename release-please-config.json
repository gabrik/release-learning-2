{   
    "packages": {
        ".": {
            "release-type": "simple",
            "extra-files": [
                {
                    "type":"toml",
                    "path":"Cargo.toml",
                    "jsonpath": "$.workspace.package.version"
                }
            ]
        },
        "clients/nodejs":{
            "release-type": "node",
            "skip-github-release": true
        },
        "clients/python":{
            "release-type": "python",
            "skip-github-release": true
        }

    },
    "release-label": "Release Learning",
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/v16.4.0/schemas/config.json"
}